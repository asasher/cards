{
  "name": "web",
  "private": true,
  "type": "module",
  "imports": {
    "#/*": "./src/*"
  },
  "scripts": {
    "dev:port": "node ../../scripts/dev/resolve-dev-app-port.mjs",
    "gateway:up": "node ../../scripts/dev/gateway-up.mjs",
    "route:generate": "tsr generate",
    "dev": "node ../../scripts/dev/run-web-dev.mjs",
    "dev:with-logs": "node ../../scripts/dev/run-web-dev.mjs",
    "build": "bun run route:generate && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "typecheck": "bun run route:generate && tsc --noEmit",
    "check": "bun run lint && bun run typecheck && bun run build",
    "db:up": "node ../../scripts/dev/db-up.mjs",
    "db:logs": "node ../../scripts/dev/db-logs.mjs",
    "db:generate": "drizzle-kit generate --config=drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config=drizzle.config.ts",
    "db:studio": "drizzle-kit studio --config=drizzle.config.ts"
  },
  "dependencies": {
    "@elysiajs/eden": "^1.4.1",
    "@t3-oss/env-core": "^0.13.8",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-devtools": "^0.7.0",
    "@tanstack/react-form": "^1.24.2",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-query-devtools": "^5.83.0",
    "@tanstack/react-router": "^1.132.0",
    "@tanstack/react-router-devtools": "^1.132.0",
    "@tanstack/react-router-ssr-query": "^1.131.7",
    "@tanstack/react-start": "^1.132.0",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.12",
    "@tanstack/router-plugin": "^1.132.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.5",
    "dotenv": "^17.2.2",
    "elysia": "^1.4.16",
    "lucide-react": "^0.545.0",
    "postgres": "^3.4.7",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.18",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@tanstack/router-cli": "^1.161.1",
    "@tanstack/devtools-vite": "^0.3.11",
    "@tanstack/eslint-config": "^0.3.0",
    "@types/node": "^22.10.2",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@vitejs/plugin-react": "^5.0.4",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.35.0",
    "prettier": "^3.5.3",
    "typescript": "^5.7.2",
    "vite": "^7.1.7",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
